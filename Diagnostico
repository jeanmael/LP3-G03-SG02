package javaapplication1;

/**
 *
 * @author Alumno
 */
public class Diagnostico {
    public void aptoVacunaA(Perro P1){
        double tmp = P1.getTemp();
        int edad = P1.getEdad();
        double peso = P1.peso;
        
        
        if (tmp >=35 && tmp<=40 && peso>5 && edad >1)
            System.out.println("Perro sano");
        else
            System.out.println("Perro enfermo");
    }
    
}
